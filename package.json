{
  "name": "@schibstedspain/sui-components",
  "version": "1.2.0",
  "description": "Mono repo containing all SUI components. Multi package for every SUI component.",
  "private": true,
  "scripts": {
    "build": "sui-studio build",
    "check:release": "sui-studio check-release",
    "co": "sui-studio commit",
    "commitmsg": "validate-commit-msg",
    "deploy": "sui-deploy spa sui-components --now -p",
    "generate": "sui-studio generate -S schibstedspain",
    "lint": "npm run lint:js && npm run lint:sass",
    "lint:js": "sui-lint js",
    "lint:sass": "sui-lint sass",
    "phoenix": "npx @s-ui/mono phoenix && (cd demo && npx @s-ui/mono phoenix)",
    "phoenix:ci": "npx @s-ui/mono phoenix --no-progress && (cd demo && npx @s-ui/mono phoenix --no-progress)",
    "precommit": "sui-precommit run",
    "release": "sui-studio release",
    "start": "sui-studio start",
    "test": "jest"
  },
  "repository": {},
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@s-ui/deploy": "2",
    "@s-ui/precommit": "2",
    "@s-ui/studio": "5",
    "babel-jest": "20.0.3",
    "enzyme": "3.3.0",
    "enzyme-adapter-react-16": "1.1.1",
    "husky": "0.13.4",
    "jest": "23.5.0",
    "validate-commit-msg": "2.12.2"
  },
  "dependencies": {
    "@s-ui/component-dependencies": "1",
    "@schibstedspain/cf-theme": "1",
    "@schibstedspain/fc-theme": "10",
    "@schibstedspain/ij-theme": "1",
    "@schibstedspain/mt-theme": "4",
    "@schibstedspain/nc-theme": "1",
    "@schibstedspain/vb-theme": "1"
  },
  "config": {
    "sui-studio": {
      "font": "https://fonts.googleapis.com/css?family=Open+Sans:400,400i,700,700i",
      "name": "SUIComponents",
      "compliant": [
        "atom/badge",
        "atom/button",
        "atom/helpText",
        "atom/image",
        "atom/panel",
        "atom/spinner",
        "atom/switch",
        "atom/tag",
        "atom/tooltip",
        "atom/validationText",
        "breadcrumb/basic",
        "molecule/notification",
        "molecule/tabs",
        "molecule/thumbnail",
        "molecule/collapsible"
      ]
    },
    "sui-mono": {
      "access": "public",
      "packagesFolder": "components",
      "deepLevel": 2
    },
    "validate-commit-msg": {
      "types": "@s-ui/mono/src/types"
    }
  },
  "commit-msg": [
    "validate-commit-msg"
  ],
  "eslintConfig": {
    "extends": [
      "./node_modules/@s-ui/lint/eslintrc.js"
    ]
  },
  "sasslintConfig": "./node_modules/@s-ui/lint/sass-lint.yml"
}
